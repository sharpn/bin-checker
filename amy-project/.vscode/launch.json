{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Backfill",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/app/scripts/import.ts",
      "outFiles": ["${workspaceFolder}/app/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Backfill Products",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/app/scripts/import-products.ts",
      "outFiles": ["${workspaceFolder}/app/**/*.js"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/app/server.ts",
      "outFiles": ["${workspaceFolder}/app/**/*.js"]
    }
  ]
}
